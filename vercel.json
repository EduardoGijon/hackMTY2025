{
  "buildCommand": "prisma generate && next build",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-1.0.x,rhel-openssl-3.0.x"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_SKIP_DOWNLOAD": "false",
      "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-1.0.x,rhel-openssl-3.0.x"
    }
  }
}